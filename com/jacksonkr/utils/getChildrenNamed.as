package com.jacksonkr.utils {	import flash.display.DisplayObjectContainer;	import flash.display.Shape;	public function getChildrenNamed(paren:*, name:String):Array {		var arr:Array = [];				if(paren.numChildren) {			for(var i = 0; i < paren.numChildren; ++i) {				var child:* = paren.getChildAt(i);				if(child.name == name) arr.push(child);								var nc:int = 0;				try {nc = child.numChildren } catch(e:Error) {};								if(nc) {					arr = arr.concat(getChildrenNamed(child, name));				}			}		}				return arr;	}}